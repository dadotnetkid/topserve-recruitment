                <div class="form-group">
                    @Html.DropDownList("month", Dropdown_list_data.Monthly(), "Select Month", new { @class = "form-control input-sm", required = "" })
                </div>
            </div>

            <div class="col-xs-12 col-sm-12 col-lg-4">
                <div class="form-group">
                    @Html.DropDownList("year", Dropdown_list_data.Yearly(), "Select Year", new { @class = "form-control input-sm", required = "" })
                </div>
            </div>

            <div class="col-xs-12 col-sm-12 col-lg-4">
                <div class="btn-group">
                    <button class="btn btn-default btn-default-red" type="button" onclick="ApplicantSurveySummaryReport('form', '@Url.Action("EducationalAttainmentReport")', '@Url.Action("HowDidYouKnowTopserveReport")', '@Url.Action("InvitedByReport")', '@Url.Action("JobFairReport")', '#educationattainment', '#howdidyouknowtopserve','#invitedby','#jobfair','@Url.Content("~/images/loading.gif")')">Generate</button>
                    <button class="btn btn-default btn-default-red" type="button" onclick="ExportApplicantSurvey('form','@Url.Action("ExportApplicantSurveySummary")','#month','#year')">Download</button>
