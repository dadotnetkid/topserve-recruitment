@using RecruitmentSystem.Recruitment.Data;
@{
    ViewBag.Title = "Applicant Survey Summary";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row">
    <div class="col-lg-12">
        <h3>Applicant Survey Summary Report</h3>
    </div>
</div>
<div class="row">
