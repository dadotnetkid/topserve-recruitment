@model IEnumerable<RecruitmentSystem.Recruitment.Data.sp_manpower_audit_trail_listResult>
@{
    Layout = null;
}

<table class="table table-striped table-bordered table-condensed ">
    <thead>
        <tr>
            <th>Name</th>
            <th>Action</th>
            <th>Date Created</th>
        </tr>
    </thead>
    <tbody>
        @if(Model.Count()>0)
        {
            foreach(var i in Model)
            {
                <tr>
                    <td>@i.createdby</td>
                    <td>@i.trail</td>
                    <td>@i.date_created</td>
                </tr>
            }
        }
        else
        {
            <tr>
                <td colspan="4" align="center">No Audit Trail</td>
            </tr>
        }
    </tbody>
</table>