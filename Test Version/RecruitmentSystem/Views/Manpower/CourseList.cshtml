@model IEnumerable<RecruitmentSystem.Recruitment.Data.sp_course_listResult>
@{
    Layout = null;
}
<tr hidden>
    <td>
        <strong id="count-result">@ViewBag.count</strong>
    </td>
</tr>
@if (Model.Count() == 0)
{
    <tr>
        <td colspan="2">No Result Found</td>
    </tr>
}
else
{
    foreach (var i in Model)
    {
        <tr>
            <td><input type="radio" name="radioname" data-text="@i.course_name" value="@i.course_id" /></td>
            <td>@i.course_name</td>
        </tr>
    }
}