@model IEnumerable<RecruitmentSystem.Recruitment.Data.sp_archieved_listResult>
    @{
        ViewBag.Title = "Import Manpower Request Achieve Index";
        Layout = "~/Views/Shared/_Layout.cshtml";
    }

    <div class="row">
        <div class="col-lg-12">
            <h3>Import MRF Achieve</h3>
        </div>
    </div>
    <div class="row">
        <div class="col-lg-12">
            <ol class="breadcrumb">
                <li> <a href="@Url.Action("dashboard","manpower")">Dashboard</a></li>
                <li class="active"> Import MRF Achieve</li>
            </ol>
        </div>
    </div>

    <div class="row">
        <div class="col-lg-12">
            <div class="panel panel-default panel-default-red">
                <div class="panel-heading"><h3 class="panel-title">Import MRF Archieve</h3></div>
                <div class="panel-body">
                    <div class="row">
                        <div class="col-lg-12">
                            <div class="btn-group pull-right ">
                                <button class="btn btn-default btn-default-red" data-toggle="modal" data-target="#modal-import-applicant">Import Applicant</button>
                                <button class="btn btn-default btn-default-red" data-toggle="modal" data-target="#modal-import">Import MRF</button>

                            </div>
                            <div class="clearfix"></div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-lg-12">
                            <table class="table table-border-less table-hover table-small-font table-striped">
                                @foreach(var i in Model )
                                {
                                    <tr>
                                        <td width="50%">@i.mrfid</td>
                                        <td width="50%">@i.RequiredPosition </td>
                                    </tr>
                                }
                            </table>
                        </div>
                    </div>
                </div>
            </div>

        </div>
    </div>
    <div class="modal fade" id="modal-import">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header modal-header-danger">
                    <h4 class="modal-title">Import MRF Archieve</h4>
                </div>
                <form method="post" enctype="multipart/form-data" action="@Url.Action("ImportManpowerRequestAchieveIndex")" id="form-upload">

                    <div class="modal-body">
                        <input type="file" name="file" class="file-hidden" id="file" onchange="onchangefilebrowse(this, '#filename', '#form-upload')" />
                        <input type="text" class="form-control  " id="filename" placeholder="Filename" onclick="clickfilebrowse('#file')" />
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-default " data-dismiss="modal">Close</button>
                        <button type="button" class="btn btn-default btn-default-red" onclick="clickfilebrowse('#file')">Import</button>
                    </div>
                </form>
            </div>
        </div>
    </div>
<div class="modal fade" id="modal-import-applicant">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header modal-header-danger">
                <h4 class="modal-title">Import Applicant Archieve</h4>
            </div>
            <form method="post" enctype="multipart/form-data" action="@Url.Action("ImportApplicantArchieve")" id="form-upload-applicant">

                <div class="modal-body">
                    <input type="file" name="fileapplicant" class="file-hidden" id="fileapplicant" onchange="onchangefilebrowse(this, '#filenameapplicant', '#form-upload-applicant')" />
                    <input type="text" class="form-control  " id="filenameapplicant" placeholder="Filename" onclick="clickfilebrowse('#fileapplicant')" />
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-default " data-dismiss="modal">Close</button>
                    <button type="button" class="btn btn-default btn-default-red" onclick="clickfilebrowse('#fileapplicant')">Import</button>
                </div>
            </form>
        </div>
    </div>
</div>
