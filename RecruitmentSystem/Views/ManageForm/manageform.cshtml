@{
    ViewBag.Title = "manageform";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row">
    <div class="col-lg-12">
        <h3>Manage Form</h3>
    </div>
</div>
<div class="row">
    <div class="col-lg-12">
        <ol class="breadcrumb">
            <li><a href="@Url.Action("dashboard","home")">Dashboard</a></li>
            <li class="active">Manage Form</li>
        </ol>
    </div>
</div>
<div class="row">
    <div class="col-lg-12 " id="#alert-container">
        <div class="alert alert-success" hidden="hidden"></div>
    </div>
</div>
@if (ViewBag.status != "")
{
    <div class="row">
        <div class="col-lg-12 " id="#alert-container">
            <div class="alert alert-success" >@ViewBag.status</div>
        </div>
    </div>
}
<div class="row">
    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
        <h3>Manage Form</h3>
        <div class="row">
            <div class="col-xs-12">
                <ol class="breadcrumb">
                    <li>Dashboard</li>
                    <li class="li-active"> Manage Form</li>
                </ol>
            </div>
        </div>
        <div class="well custom-panel">
            <div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                    <strong class="vertical-align">Manage Form</strong>
                    <div class="input-group pull-right">
                        <div class="btn-group">
                            <a class="btn dropdown-toggle custom-button" data-toggle="dropdown" href="#"> <span class="fa fa-ellipsis-v fa-lg" title="Toggle dropdown menu"></span> </a>
                            <ul class="dropdown-menu dropdown-menu-right">
                                <li><a href="#" data-target="#add-modal" data-toggle="modal"  id="add-new"><i class="fa fa-plus fa-fw" aria-hidden="true"></i> Create</a></li>
                                <li class="divider"></li>
                                <li><a href="#" data-target="#import-modal" data-toggle="modal" ><i class="fa fa-upload fa-fw"></i> Import</a></li>
                            </ul>
                        </div>
                    </div>
                    <p class="empty-space-12"></p>
                </div>
            </div>
            <div class="row">
                <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                    <ul class="nav nav-tabs nav-tabs-custom nab-tabs-black" role="tablist">
                        <li role="presentation" class="active"><a href="#skills" aria-controls="home" role="tab" data-toggle="tab" data-image-url="@Url.Content("~/images/loading.gif")" onclick="manageform('@Url.Action("skills")','#skills',this)">Skills</a></li>
                        <li role="presentation"><a href="#certificates" aria-controls="certificates" role="tab" data-toggle="tab" data-image-url="@Url.Content("~/images/loading.gif")" onclick="manageform('@Url.Action("certificates")','#certificates',this)">Certificates</a></li>
                        <li role="presentation"><a href="#positions" aria-controls="positions" role="tab" data-toggle="tab" data-image-url="@Url.Content("~/images/loading.gif")" onclick="manageform('@Url.Action("positions")','#positions',this)">Positions</a></li>

                    </ul>
                    <div class="tab-content">
                        <div role="tabpanel" class="tab-pane active scrollable " id="skills">@Html.Action("skills")</div>
                        <div role="tabpanel" class="tab-pane scrollable" id="certificates"></div>
                        <div role="tabpanel" class="tab-pane scrollable" id="positions"></div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    
</div>
