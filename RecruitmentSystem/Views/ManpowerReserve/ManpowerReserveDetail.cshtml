@model RecruitmentSystem.Recruitment.Data.sp_manpower_reserve_detailResult
@{
    Layout = null;
    var i = Model;
}
<div class="row">
    <div class="col-lg-12">
   
            <i class="fa fa-pencil-square-o" aria-hidden="true"></i><strong> Requirement / Qualification</strong>
            <div class="row">
                <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                    <div class="row">
                        <div class="col-xs-12 col-sm-12 col-lg-6">
                            <p class="detail-margin"><strong class="pull-left">Position</strong></p>
                        </div>
                        <div class="col-xs-12 col-sm-12 col-lg-6">
                            <p class="detail-margin"><span class="pull-left">@i.position_name</span></p>
                        </div>
                    </div>
                </div>
                <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                    <div class="row">
                        <div class="col-xs-12 col-sm-12 col-lg-6">
                            <p class="detail-margin"><strong class="pull-left">Education</strong></p>
                        </div>
                        <div class="col-xs-12 col-sm-12 col-lg-6">
                            <p class="detail-margin"><span class="pull-left">@i.educationalattainment</span></p>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                    <div class="row">
                        <div class="col-xs-12 col-sm-12 col-lg-6">
                            <p class="detail-margin"><strong class="pull-left">Course</strong></p>
                        </div>
                        <div class="col-xs-12 col-sm-12 col-lg-6">
                            <p class="detail-margin"><span class="pull-left">@i.course_name</span></p>
                        </div>
                    </div>
                </div>
                <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                    <div class="row">
                        <div class="col-xs-12 col-sm-12 col-lg-6">
                            <p class="detail-margin"><strong class="pull-left">Gender</strong></p>
                        </div>
                        <div class="col-xs-12 col-sm-12 col-lg-6">
                            <p class="detail-margin"><span class="pull-left">@i.gender</span></p>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                    <div class="row">
                        <div class="col-xs-12 col-sm-12 col-lg-6">
                            <p class="detail-margin"><strong class="pull-left">Age Requirement</strong></p>
                        </div>
                        <div class="col-xs-12 col-sm-12 col-lg-6">
                            <p class="detail-margin"><span class="pull-left">@i.agerequirement</span></p>
                        </div>
                    </div>
                </div>
                <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                    <div class="row">
                        <div class="col-xs-12 col-sm-12 col-lg-6">
                            <p class="detail-margin"><strong class="pull-left">Skills</strong></p>
                        </div>
                        <div class="col-xs-12 col-sm-12 col-lg-6">
                            <p class="detail-margin"><span class="pull-left">@i.skill_name</span></p>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                    <div class="row">
                        <div class="col-xs-12 col-sm-12 col-lg-6">
                            <p class="detail-margin"><strong class="pull-left">Certification</strong></p>
                        </div>
                        <div class="col-xs-12 col-sm-12 col-lg-6">
                            <p class="detail-margin"><span class="pull-left">@i.certificate_name</span></p>
                        </div>
                    </div>
                </div>
            </div>

    
        
    </div>
</div>
